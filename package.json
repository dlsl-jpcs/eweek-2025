{
    "name": "eweek-2025-game",
    "version": "1.0.0",
    "description": "E-Week 2025 Game with Admin Approval System",
    "main": "server/server.js",
    "scripts": {
      "start": "node server/server.js",
      "build": "cd client && npm install --include=dev && npm run build",
      "install-client": "cd client && npm install --include=dev",
      "install-server": "cd server && npm install",
      "install-all": "npm run install-client && npm run install-server",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "server": "cd server && npm run dev",
      "client": "cd client && npm run dev"
    },
    "keywords": ["game", "react", "node", "express"],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "concurrently": "^8.2.2"
    }
  }